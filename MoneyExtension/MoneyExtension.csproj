<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<PackageId>MoneyExtensionsConverter</PackageId>
		<Version>1.0.0</Version>
		<License>MIT</License>
		<Authors>Luiz Goebel</Authors>
		<Company>Luiz.Goebel</Company>
		<PackageDescription>Extension to convert decimal to integer</PackageDescription>
		<Description>Extension to convert decimal to integer</Description>
		<RepositoryUrl>https://github.com/luizgoebel/money-extensions</RepositoryUrl>
		<PackageProjectUrl>https://github.com/luizgoebel/money-extensions</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageIcon>icon.png</PackageIcon>
		<PackageTags>Money;int;decimal;converter</PackageTags>
		<Title>MoneyExtensionsConverter</Title>
		<Summary>Extension to convert decimal to integer</Summary>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\Media\icon.png" Pack="true" PackagePath="\"/>
	</ItemGroup>
</Project>
